package JavaBase;

public class DataTypes {

	public static void main(String[] args) {
		
		//1. Interger int:
		int i = 10;
		int j = 20;
		int k =30;
		
		//2. double:
		double d = 12.33;
		double d1 = 34.14;
		double d2 = 100;

		//3. Character c:
		char c = 'a';
		char b = 'b';
		char a = 'c';
		
		//4. Boolean
		boolean b1 = true;
		boolean b2 = false;
		
		//5. String ( String is a class)
		String s1 = "Java";
		String s2 = "Java Basics";
	
	}

}
