package JavaBase;

import java.util.Hashtable;

public class HashTable {

	public static void main(String[] args) {

		Hashtable h = new Hashtable();
		h.put("A", "Jhon");
		h.put("B", "Doe");
		h.put(10, 28);
	
		System.out.println(h.size());
		
		h.put(1, 100);
		h.put(2,200);
		
		System.out.println(h.get("A"));
	}

}
