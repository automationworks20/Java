package JavaBase;

public class TwoDimArray {

	public static void main(String[] args) {

		String x[][] = new String[3][5];
		System.out.println(x.length); // total no. of rows = 3
		System.out.println(x[0].length); // total no. of columns = 5
		
		x[0][0] = "Hello";
		x[0][1] = "World";
		x[0][2] = "1";
		
	}

}
