package OOPConceptsPart1;

public class Car {

	//Class variables:
	int model;
	int wheel;
	
	public static void main(String[] args) {
		
		Car a = new Car();
		Car b = new Car();
		Car c = new Car();
		
		a.model = 2022;
		a.wheel = 4;
		
		b.model = 2021;
		b.wheel = 4;
		
		c.model = 2020;
		c.wheel = 4;
		
		System.out.println(a.model);
		System.out.println(b.wheel);
		System.out.println(c.model);
	}

}
